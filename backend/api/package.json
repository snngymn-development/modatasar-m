{
  "name": "@deneme1/api",
  "version": "0.1.0",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "openapi:emit": "node ./src/swagger.emit.js",
    "codegen:rn": "openapi-generator-cli generate -i openapi.json -g typescript-axios -o ../../frontend/mobile/generated --additional-properties=supportsES6=true",
    "codegen:all": "pnpm openapi:emit && pnpm codegen:rn"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/websockets": "^11.1.6",
    "@prisma/client": "^5.18.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.6.0",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.20.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.0",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.6",
    "@openapitools/openapi-generator-cli": "^2.13.0",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^6.0.0",
    "jest": "^29.5.0",
    "prisma": "^5.18.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
